{
  "name": "@mondomember/test",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "version": "1.10.1",
  "scripts": {
    "analyse": "yarn run lint && yarn run test",
    "build:cjs": "tsc -d -p ./tsconfig.json",
    "build": "yarn build:cjs",
    "lint": "eslint . --ext .ts",
    "test": "jest --passWithNoTests",
    "test:ci": "jest --passWithNoTests -i",
    "watch": "tsc -w",
    "version": "tsc -v"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mondomember/node.git",
    "directory": "packages/test"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Flipbox Digital",
    "url": "https://flipboxdigital.com"
  },
  "license": "MIT",
  "dependencies": {
    "@mondomember/sdk": "^1.10.1",
    "chance": "^1.1.8",
    "stripe": "^9.8.0"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.1",
    "@types/chance": "^1.1.2",
    "@types/jest": "^27.0",
    "@types/node": "^14.0.27",
    "jest": "^27.0",
    "ts-jest": "^27.1.4",
    "ts-node": "^8.10.2",
    "typescript": "^4.0.0"
  },
  "engines": {
    "node": ">=14"
  },
  "jest": {
    "verbose": true,
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test))\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "gitHead": "ad62bbf3bf0d4acafc409f9f72e2fcccb4de9a0f"
}
